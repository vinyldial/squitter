{
  "manifest_version": 2,
  "name": "Squitter",
  "description": "A Chrome extension to restore square avatars on Twitter",
  "version": "0.0.3",

  "icons": {
          "128": "squitter-icon.png"
        },

  "content_scripts": [
    {
      "matches": ["https://twitter.com/*"],
      "css": ["css/squitter.css"]
    }
  ]
}
